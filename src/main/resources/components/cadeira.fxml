<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/17.0.15" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="cadeira" mnemonicParsing="false" onAction="#eventClick" onMouseEntered="#mudarCor" onMouseExited="#voltarCor" prefHeight="60.0" prefWidth="100.0" style="-fx-background-radius: 8; -fx-background-color: #DCDCDC;" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="11.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</fx:root>
